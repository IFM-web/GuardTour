
@{
    ViewData["Title"] = "PhotoAttendance";
  Layout = "~/Views/Shared/_layoutdashboard.cshtml"; 
}
 

        <!-- partial -->
<div class="main-panel main-panel-one" style="width:100% !important">
            <div class="content-wrapper pt-0">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="home-tab">
                            <div class="tab-content tab-content-basic pt-0">
                                <div class="tab-pane fade show active" id="overview" role="tabpanel" aria-labelledby="overview">
                                    <div class="row">
                                        <div class="col-lg-12 d-flex flex-column">
                                            <div class="row flex-grow">
                                                <div class="col-12 grid-margin stretch-card">
                                                    <div class="card card-rounded">
                                                        <div class="card-body">
                                                            <div class="d-sm-flex justify-content-between align-items-start">
                                                                <div>
                                                                    <h4 class="card-title card-title-dash">
                                                                        IAM360 Reports / <a href="#">
                                                                            Photo
                                                                            Attendance
                                                                        </a>
                                                                    </h4>
                                                                </div>
                                                            </div>
                                                            <br>
                                                            <div class="row">
                                                                <div class="col-md-10 dis-form">
                                                                    <div class="data-input">
                                                                        <div class="row">
                                                                    <div class="col-md-4">
                                                                        <div class="nav-item d-lg-block">
                                                                            <div id="datepicker-popups"
                                                                                 class="input-group date datepicker navbar-date-picker">
                                                                                <span class="input-group-addon input-group-prepend border-right">
                                                                                    <span class="icon-calendars input-group-text calendar-icon"><img src="~/img/calendar.png"> <span class="icon-date">Date</span></span>
                                                                                </span>
                                                                                <input type="text" class="form-control" id="date-input">
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                            <div class="col-md-5">
                                                                                <div class="nav-item d-none d-lg-block">
                                                                                    <div id="datepicker-popups"
                                                                                            class="input-group date datepicker navbar-date-picker">
                                                                                        <span class="input-group-addon input-group-prepend border-right">
                                                                                            <span class="icon-calendars input-group-text calendar-icon"><img src="~/img/Login-Icons.png"> <span class="icon-date">Employee No</span></span>
                                                                                        </span>
                                                                                        <input id="Empnumber" type="text" placeholder="Employee Number" class="form-control">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-2 dis-form">
                                                            <button onclick="SearchData()" class="btn-btn">
                                                                        View
                                                                    </button>
                                                                </div>

                                                                <div class="col-md-12">
                                                                    <div class="table-data">
                                                                        <table class="employee-table">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>S.No</th>
                                                                                    <th>Employee No</th>
                                                                                    <th>Employee Name</th>
                                                                                    <th>Shift Details</th>
                                                                                    <th>Status</th>
                                                                                    <th>Date</th>
                                                                                    <th>Time</th>
                                                                                    <th>Latitude</th>
                                                                                    <th>Longitude</th>
                                                                                    <th style="width: 240px;">Location</th>
                                                                                    <th>Photo</th>
                                                                                </tr>
                                                                            </thead>
                                                                    <tbody class="companybody">
                                                                               
                                                                                  
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-2">
                                                                    <button class="btn-btn" onclick="ExportExcel()" style="background:#00ac60;padding: 10px 0px;">
                                                                        Export to Excel
                                                                    </button>
                                                                </div>
                                                                <div class="col-md-2">
                                                                    <button class="btn-btn" style="background:#001C35;padding: 10px 0px;">
                                                                        Export to PDF
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        
        </div>
        <!-- main-panel ends -->

        <script src="~/js/site.js"></script>
        <script src="~/js/xlxs.full.min.js"></script>
